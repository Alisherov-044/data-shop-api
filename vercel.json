{
    "version": 2,
    "env": {
        "PORT": "@PORT",
        "DATABASE_URL": "@DATABASE_URL",
        "DATABASE_URL_UNPOOLED": "@DATABASE_URL_UNPOOLED",
        "JWT_SECRET": "@JWT_SECRET"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PATCH", "DELETE"]
        }
    ]
}
